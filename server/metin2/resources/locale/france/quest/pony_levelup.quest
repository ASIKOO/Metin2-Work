			say(locale.NOTICE_COLOR.."Une médaille équestre"..locale.NORMAL_COLOR)
			say("")
			---                                                   l
			say("Pour continuer l'entrainement de votre cheval,")
			say("vous devez possedez des médailles équestre, ")
			say("vous en trouverez en tuant les singe des ")
			say("differents donjons.")
			say("")
			setstate(start)
			q.done()
		end
	end
	state failure begin
		when letter begin
			setskin(NOWINDOW)
			makequestbutton("Vous avez echoué ")
			q.set_title("Vous avez echoué ")
			q.start()
		end
		when button or info begin
			say(locale.NOTICE_COLOR.."Vous avez echoué "..locale.NORMAL_COLOR)
			say("")
			say("Vous etes descendu de votre cheval, vous")
			say("avez donc echoué, re-essayer plus tard.")
			say("")
			setstate(start)
		end
	end
	state report begin
		when letter begin
			local v=find_npc_by_vnum(20349)
			if 0==v then
			else
				target.vid("__TARGET__", v, "")
			end
		end
		when letter begin
			setskin(NOWINDOW)
			makequestbutton("Allez voir le palefrenier")
			q.set_title("Allez voir le palefrenier")
			q.start()
		end
		when button or info begin
			say(locale.NOTICE_COLOR.."Allez voir le palefrenier"..locale.NORMAL_COLOR)
			say("")
			say("Vous avez terminé votre entrainement.")
			say("Allez le dire au palefrenier afin de")
			say("connaitre votre resultat")
			say("")
		end
		when __TARGET__.target.click begin
			target.delete("__TARGET__")
			say("Palefrenier :")
			say("")
			say("Vous avez terminé votre entrainement ?")
			say("C'est du bon travail")
			say("Je vais vous donner votre resultat.")
			say("")
			setstate(reward)
		end
	end
	state reward begin
		when letter begin
			setskin(NOWINDOW)
			makequestbutton("Résultat de l'entrainement")
			q.set_title("Résultat de l'entrainement")
			q.start()
		end
		when button begin
			say(locale.NOTICE_COLOR.."Résultat de l'entrainement"..locale.NORMAL_COLOR)
			if horse.get_level()>=11 then
				say("Votre cheval est deja level 11")
			elseif (horse.get_level()>=1 and horse.get_level()<=10) then
				local s=number(1, 2)
				if 1>=s then -- 50.00% (1/2)
					say("")
					say("Vous avez échoué à cet entrainement !")
					say("")
					say("Re-essayer une prochaine fois.")
					pc.setqf("next_time", 0)
				elseif 2>=s then -- 50.00% (1/2)
					say("")
					if pc.countitem("50050")>=1 then
						char_log(0, "PONY_ADVANCE", "BEGIN")
						pc.removeitem("50050", 1)
						char_log(0, "PONY_ADVANCE", "DEC 50050 1")
						horse.unride()
						horse.advance()
						horse.ride()
						char_log(0, "PONY_ADVANCE", "INC horse_advance 1")
						char_log(0, "PONY_ADVANCE", "END")
						say("Votre cheval a atteint le niveau "..(horse.get_level())..".")
						say("")
						if horse.get_level()==10 then
							say("Votre cheval est à son niveau maximum.")
							say("Vous devriez l'améliorer.")
							pc.setqf("next_time", 0)
						else
							say("Vous devriez continuez d'entrainez")
							say("votre cheval jusqu'au niveau 10.")
							say("Votre prochain entrainement pourra débuter")
							say("dans 2 heures.")
							say("")
						end
					else
						say("Il vous faut une médaille équestre pour")
						say("inscrire votre resultat.")
						say("")
					end
				end
			end
			setstate(start)
			q.done()
		end
	end
	state __COMPLETE__ begin
		when enter begin
			q.done()
		end
	end
end